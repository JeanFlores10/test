pipeline {
    agent any
    stages {
        stage("build"){
            steps{
                echo 'building the application'
                echo "SECRET_URL = ${env.GIT_URL}eee"
                echo "URL_TEST = ${env.URL_TEST}test/hello" 
                echo "BUILD_NUMBER = ${env.BUILD_NUMBER}"
            }
        }

        stage("test"){
            steps{
                echo 'testing the application'
            }
        }

        stage("deploy"){
            steps{
                echo 'deplying the application'
            }
        }

    }
}